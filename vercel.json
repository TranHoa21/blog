{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/v1/"
        }
    ],
    "env": {
        "NODE_ENV": "production",
        "DATABASE_URL": "mongodb://localhost:27017/blog"
    },
    "public": "public"
}